.item-title{
  display: flex;
  flex-direction: row;
  align-items: center;
}

.icon-css{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 30rpx;
  height: 30rpx;
  border-radius: 30rpx;
  color: #fff;
  font-size: 24rpx;
}

.comm-pro-item{
  display: flex;
  flex-direction: row;
  align-items: center;
}

.numberImg{
  width: 17px;
  height: 17px;
  font-size: 10px;
  color: white;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 3px;
}

.wrapper {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
}

.block {
  width: 320px;
  height: 500px;
  background-color: #fff;
  margin: 7px 0;
}

